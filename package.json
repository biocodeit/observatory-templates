{
  "name": "@junobuild/observatory-templates",
  "private": true,
  "version": "0.2.1",
  "scripts": {
    "build": "email build",
    "dev": "email dev",
    "export": "email export --outDir html && email export --plainText --outDir text",
    "format": "prettier --write .",
    "test": "NODE_ENV=development playwright test",
    "test:snapshots": "NODE_ENV=development playwright test --update-snapshots --reporter=list",
    "test:ci": "playwright test --reporter=html",
    "test:ci:snapshots": "playwright test --update-snapshots --reporter=html"
  },
  "dependencies": {
    "@react-email/components": "0.0.40",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.4",
    "prettier": "^3.5.3",
    "react-email": "^4.0.13"
  }
}
